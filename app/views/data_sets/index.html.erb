<p id="notice"><%= notice %></p>

<h1>Data Sets</h1>

<table>
  <thead class="table table-striped">
    <tr>
      <th>Name</th>
      <th>Production</th>
      <th>Data starts</th>
      <th>Data ends</th>
      <th>Data source</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_sets.each do |data_set| %>
      <tr>
        <td><%= data_set.name %></td>
        <td><%= data_set.production %></td>
        <td><%= data_set.data_starts %></td>
        <td><%= data_set.data_ends %></td>
        <td><%= data_set.data_source %></td>
        <td><%= link_to 'Show', data_set %></td>
        <td><%= link_to 'Edit', edit_data_set_path(data_set) %></td>
        <td><%= link_to 'Destroy', data_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Set', new_data_set_path %>
